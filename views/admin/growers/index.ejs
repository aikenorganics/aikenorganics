<main class='container'>
  <% include ../subnav %>
  <h1>Growers</h1>
  <hr>
  <table class='table'>
    <thead>
      <tr>
        <th>Name</th>
        <th>Active</th>
      </tr>
    </thead>
    <tbody>
      <% growers.forEach(function(grower) { %>
        <tr>
          <td><a href='/admin/growers/<%= grower.id %>'><%= grower.name %></a></td>
          <td><% include active %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</main>
