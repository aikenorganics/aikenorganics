<main class='container'>

  <% include ../subnav %>

  <div class='row'>
    <div class='col-md-3'>
      <h3>
        Growers
        <% if (locals.admin) { %>
          <a class='btn btn-default btn-xs' href='/growers/new'>
            New Grower
          </a>
        <% } %>
      </h3>
    </div>
    <div class='col-md-9'>
      <% growers.forEach(function(grower) { %>
        <% include grower %>
      <% }); %>
    </div>
  </div>

</main>
