{
  "name": "aikenorganics",
  "version": "1.0.0",
  "description": "aikenorganics.com",
  "main": "index.js",
  "cacheDirectories": [
    "node_modules"
  ],
  "engines": {
    "node": "5.5.0",
    "npm": "3.7.1"
  },
  "scripts": {
    "build": "./bin/build",
    "dump": "heroku local:run ./bin/dump",
    "postinstall": "./bin/postinstall",
    "start": "./bin/compile-js && env $(cat .env | xargs) node index.js",
    "test": "NODE_ENV=test heroku local:run --env=.testenv ./bin/test",
    "reset": "heroku local:run ./bin/reset"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aikenorganics/aikenorganics.git"
  },
  "author": "Brad Dunbar <dunbarb2@gmail.com> (http://braddunbar.net/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aikenorganics/aikenorganics/issues"
  },
  "homepage": "https://github.com/aikenorganics/aikenorganics",
  "babel": {
    "plugins": [
      "transform-object-assign"
    ],
    "presets": [
      "es2015",
      "react"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "autoprefixer": "6.3.1",
    "babel-plugin-transform-object-assign": "6.3.13",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-register": "6.4.3",
    "babelify": "7.2.0",
    "bootstrap": "3.3.6",
    "browserify": "13.0.0",
    "cssnano": "3.5.2",
    "es6-promise": "3.0.2",
    "isomorphic-fetch": "2.2.1",
    "jquery": "2.2.0",
    "marked": "0.3.5",
    "moment": "2.11.2",
    "multer": "1.1.0",
    "object-tojson": "1.0.1",
    "ozymandias": "github:braddunbar/ozymandias",
    "postcss-cli": "2.5.0",
    "postcss-import": "8.0.2",
    "postcss-nested": "1.0.0",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "redux": "3.2.1",
    "uglify-js": "2.6.1"
  },
  "devDependencies": {
    "deep-freeze": "0.0.1",
    "karma": "0.13.21",
    "karma-chrome-launcher": "0.2.2",
    "karma-tap": "1.0.3",
    "selenium-webdriver": "2.48.2",
    "standard": "6.0.7",
    "supertest": "1.2.0",
    "tape": "4.4.0"
  }
}
